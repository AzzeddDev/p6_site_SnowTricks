<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% if page_title is defined %} {{ page_title }} | {% endif %} SnowTricks</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}

    <!-- Bootstrap Css -->
    <link rel="stylesheet" href="https://bootswatch.com/5/flatly/bootstrap.min.css">
    <!-- END Bootstrap Css -->

    <!-- Animate Css -->
    <link rel="stylesheet" href="http://localhost/snowtricks/public/css/animate/hover.css">
    <link rel="stylesheet" href="http://localhost/snowtricks/public/css/animate/animate.css">
    <!-- END Animate Css -->

    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <!-- END Google Fonts -->

    <!-- Font Awesome -->
    <script src="https://kit.fontawesome.com/c571e46d19.js" crossorigin="anonymous"></script>
    <!-- END Font Awesome -->

    <!-- Fav-icon -->
    <link rel="icon" type="image.png" href="http://localhost/mon-blog/web/fav-icon.png">
    <!-- END Fav-icon -->

    <!-- Main CSS -->
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}" />
    {% endblock %}
    <!-- END Main CSS -->

    {% block javascripts %}
    {% endblock %}
</head>